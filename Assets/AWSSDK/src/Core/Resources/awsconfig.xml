<aws
    region="us-east-1"
    correctForClockSkew="true">
  
    <logging
        logTo="SystemDiagnostics"
        logResponses="Always"
        logMetrics="true"
        logMetricsFormat="JSON" />
  
    <s3 useSignatureVersion4="true" />

 
    <dynamoDB conversionSchema="V2">
      
    </dynamoDB>
 
 
 	<mobileAnalytics 
		sessionTimeout = "5"
		maxDBSize = "5242880"
		dbWarningThreshold = "0.9"
		maxRequestSize = "102400"
		allowUseDataNetwork = "false"
	/>

</aws>